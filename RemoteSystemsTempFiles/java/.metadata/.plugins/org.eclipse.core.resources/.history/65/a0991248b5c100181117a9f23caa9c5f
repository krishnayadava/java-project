import java.util.*;
class Linked_list {
	
	public static void main(String[] args){
		
		LinkedList l = new LinkedList();
	//	LinkedList l = new LinkedList(Collection c); kisi collection ko linked list me convert kr denge
		l.add("Ravi");  
		  l.add("Vijay");  
		  l.add("Ravi");  
		  l.add("Ajay");
		  l.addFirst("krishna");
		  l.addLast("mohan");
		  
		  
		  System.out.println(l);
		
		
	}

}
