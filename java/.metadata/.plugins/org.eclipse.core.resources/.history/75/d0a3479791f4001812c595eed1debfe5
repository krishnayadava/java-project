
public class ThreadPriority extends Thread {
	
	public void run(){
		System.out.println("child executed");
	}

}

class Mysthread{
	public static void main(String[] args){
		ThreadPriority t = new ThreadPriority();
		        t.setPriority(10);
		        t.start();
		        
		        
		        for(int i =0 ; i<=4; i++){
		        	System.out.println("main thread executed");
		        }
		        }
	}
